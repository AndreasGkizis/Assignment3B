@model IEnumerable<WebApplication1.Models.Examination>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<table class="table table-hover table-striped table-bordered m-auto">
    <tr>
        <th>
            Certificate Title
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssessmentCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExaminationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CandidateScore)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PercentageScore)
        </th>
        <th>
            Actions
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr class="text-center">
            <td>
                @Html.DisplayFor(modelItem => item.Certificate_Id.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AssessmentCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExaminationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CandidateScore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PercentageScore)%
            </td>
            <td>
                @Html.ActionLink("Print", "Print", "Candidate", new { id = item.Id }, new { @class = "btn btn-success", @target="_blank" })

            </td>
        </tr>
    }

</table>
